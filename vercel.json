{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },

    {
      "src": "/create-checkout-session",
      "dest": "/server.js"
    },
    {
      "src": "/user-email",
      "dest": "/server.js"
    },
    {
      "src": "/webhook",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|webp|ico|woff2|ttf|eot|otf)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  }
}
